expression rassoc 12 _ : _ ;
expression lassoc 9 _ _ ;


A in A ;
P in A -> P in (A ^ B) ;
P in B -> P in (A ^ B) ;

B in (A ^ KB) -> (A -> B) in KB ;

X[X/Y] = Y ;
E1[x/y] = E1' ->
  E2[x/y] = E2' ->
  (E1 E2)[x/y] = (E1' E2') ;

