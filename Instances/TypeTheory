{-
-- a in [] -> bot ;
a in (a :: xs) ;
a in xs -> a in (b :: xs) ;

(a : A) in K -> proof (a : A) from K end ;

(f : (p' : P') => Q') in K ->
  (f : (p' : P') => Q')[p'/p] is (f : (p : P) => Q) ->
  proof (p : P) from K end ->
  proof (b : B) from ((f p : Q) :: K) end ->
  proof (b : B) from K end ;

proof B from (A :: K) end ->  proof (b : A => B) from K end ;

proof P from [] end -> makeProof P ;
-}

