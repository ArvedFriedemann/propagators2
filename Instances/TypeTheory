expression nassoc 7 ( _ ) ;
expression rassoc 13 _ = _ ;
expression rassoc 15 _ -> _ ;
expression rassoc 12 _ : _ ;
expression lassoc 9 _ _ ;


x = x ;

a in (a :: xs) ;
a in xs -> a in (b :: xs) ;

x[x/y] = y ;
(e = x -> bot) -> e[x/y] = e ;

e1[x/y] = e1' ->
  e2[x/y] = e2' ->
  (e1 e2)[x/y] = (e1' e2') ;
